<%include ../partials/header.ejs%>


<div class="row">
    <%include sideBar.ejs%>
    <div class="col-lg-10 col-md-11 col-sm-9 col-xs-9 bhoechie-tab">
        <div class="bhoechie-tab-content active">
            <div class="container-fluid col-lg-6 col-md-6 col-sm-12 col-xs-12 col-lg-offset-3 col-md-offset-3 col-sm-offset-1 col-xs-offset-1">
                <div class="panel panel-primary">
                    <div class="panel-heading">
                        <h3>Profile</h3>
                    </div>
                    <div class="panel-body">

                        <form method="POST" action="/addData" class="form-validation">
                            <div class="form-group">
                                <label >Name: </label>
                                <label id="Name">Name</label>
                            </div>
                            <div class="form-group">
                                <label for="userpassold">Old password</label>
                                <input type="password" name="password" placeholder="Enter old password" class="form-control" id="userpassold">
                            </div>
                            <div class="form-group">
                                <label for="userpassnew">New password</label>
                                <input type="password" name="password" placeholder="Enter new password" class="form-control" id="userpassnew">
                            </div>
                            <div class="form-group">
                                <label for="userpassrep">Repeat password</label>
                                <input type="password" name="password" placeholder="Repeat new password" class="form-control" id="userpassrep">
                            </div>
                            <div class="form-group">
                                <button type="submit" class="btn btn-primary pull-right">Done</button>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<%include ../partials/footer.ejs%>
